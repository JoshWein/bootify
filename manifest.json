{	
	"name": "bootify - A Bootstrap Extension",
	"version": "0.71",
	"permissions": [
    	"activeTab",
    	"storage"
  	],
  	"background": {
  		"scripts": ["background.js"],
  		"persistent": false
	},	
	"browser_action": {
		"default_title": "Add bootstrap to page."
	},
	"manifest_version": 2,
	"options_page": "options.html"
}
